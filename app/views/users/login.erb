<%= erb :'partials/_navbar', layout: false%>
<div class="row">
  <div class="container">
    <form class="" action="/users/login" method="post">
      <h1>Login</h1>
      <div class="form-group">
        <label for="email">Email:</label>
        <input type="email" class="form-control" name="user[email]" value="">
      </div>
      <div class="form-group">
        <label for="password">Password:</label>
        <input type="password" class="form-control" name="user[password]" value="">
      </div>
      <div class="form-group">
          <input type="submit" class="btn btn-primary" name="" value="Log In">
      </div>
    </form>
  </div>
</div>
